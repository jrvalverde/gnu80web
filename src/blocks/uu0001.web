      data Dirtab/'DIR' , -1 , '(' , 0 , 0 , 0 , 'EOL' , 'EXI' , 1 , 
     &     -1 , 'NUL' , 'OV' , 0 , 0 , 'EOS' , 0 , 'I10' , 0 , 2 , 0 , 
     &     'EOS' , 0 , -1 , ')' , 'RET' , 1 , 1 , 'EOS'/
C                    state to deal with alternate directory.
      data Ovtab/'OV' , 'I10' , 0 , 2 , 0 , 'EOS' , 0 , -1 , '/' , 
     &     'RET' , 1 , 2 , 'EOS'/
C                    state to get overlay number.
      data Optab/'OP' , -1 , '/' , 'SEG' , 0 , 0 , 'I10' , 0 , 3 , 0 , 
     &     -1 , ',' , 'OP' , 0 , 0 , 'EOS' , 0 , -1 , '=' , 0 , 1 , 3 , 
     &     'EOS' , 0 , 'I10' , 'RET' , 2 , 0 , 'EOS' , 0 , 'NUL' , 
     &     'OP' , 0 , 0 , 'EOS'/
