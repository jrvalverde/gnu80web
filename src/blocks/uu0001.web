@a
      blockdata uu0001
      implicit none
      integer Dirtab , Dummy , First , Jmptab , Optab , Ovtab , Segtab
C for croute
      common /bd0001/ Dummy
      common /crtab / First(32) , Dirtab(28) , Ovtab(13) , Optab(35) , 
     &                Segtab(29) , Jmptab(21)
C ----------------------------------------------------------------------
C                    ***** qparse table *****
      data First/'TOP' , -1 , '#' , 'TOP' , 0 , 0 , 6 , 'NONS' , 'TD' , 
     &     'TOP' , 0 , 0 , 1 , 'N' , 'TOP' , 0 , 0 , 1 , 'P' , 'TOP' , 
     &     0 , 0 , 1 , 'F' , 'TOP' , 0 , 0 , 'NUL' , 0 , 0 , 0 , 'EOS'/
