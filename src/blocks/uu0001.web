      data Segtab/'SEG' , 'I10' , 0 , 2 , 0 , 'EOS' , 0 , 'NUL' , 
     &     'RET' , 1 , 4 , 'EOS' , 0 , -1 , '(' , 'J' , 0 , 0 , -1 , 
     &     ';' , 'DIR' , 0 , 0 , -1 , ',' , 'SEG' , 0 , 0 , 'EOS'/
C                    state to get link numbers.
      data Jmptab/'J' , 'I10' , 0 , 2 , 0 , 'EOS' , 0 , -1 , ')' , 
     &     'RET' , 1 , 5 , 'EOS' , 0 , -1 , ';' , 'DIR' , 0 , 0 , 
     &     'EOS' , 'END'/
C
C                    state to get 'jump' spec.  "(" has already
C                    been read.
C
      end
