@a
      subroutine aclear(N,A)
      implicit none
      double precision A , zero
      integer i , N
C
C     routine to clear n elements in array a.
C
      dimension A(*)
      data zero/0.0D0/
C
C     loop over the n elements.
      do 100 i = 1 , N
         A(i) = zero
 100  continue
C
C     return to caller.
      return
C
      end
