\end{description} \section*{\sf Common Variables}  \begin{description}
                                                                    
\item[Modifies]    A                                                      
\item[Uses]  A       EQ      FIVE    FOUR    LPMAX   LQMAX   ONE    
             PP      THREE                                          
                                                                    
\end{description}  \section*{\sf External Subprograms}  \begin{description}
                                                                    
\item[Calls]        NOTHING                                           
\item[Called by]   DPHNIX  PHOENI                                         
                                                                    
\end{description}
@a
      subroutine geta2
      implicit none
      double precision A , Ep , Eq , Eqsav , Exparg , Five , Four , 
     &                 One , Pexp , Pp , Ptest , qq , Qxpsav , Qxsav , 
     &                 Qysav , Qzsav , Three , Two , Xint , Zero
      integer Klcutq , Klind , Lamax , Lbmax , Lcmax , Ldmax , Lpmax , 
     &        Lpqmax , Lqmax
      common /qinfo / Eqsav(100) , Qxsav(100) , Qysav(100) , Qzsav(100)
     &                , Qxpsav(100) , Exparg , Ptest , Pexp , Ep , Eq , 
     &                Pp , Klind , Klcutq(100)
      common /a     / A(174)
      common /max   / Lamax , Lbmax , Lcmax , Ldmax , Lpmax , Lqmax , 
     &                Lpqmax
      common /int   / Zero , Xint(12)
      equivalence (One,Xint(1)) , (Two,Xint(2)) , (Three,Xint(3)) , 
     &             (Four,Xint(4)) , (Five,Xint(5))
C
C***********************************************************************
C     routine to obtain a-coefficients for computing
C     two-dimensional two-center integrals from
C     raw hermite forms.  the array is dimension (7,7,13),
C     but note that all zeroes have been omitted.
C     this routine was generated by pwrite in the spring of '76.
C***********************************************************************
C
C
C     recurrence relations for a(lp,lq,iv).
      qq = One/(Eq+Eq)
      if ( Lqmax.gt.1 ) then
         A(1) = -qq
         if ( Lqmax.gt.2 ) then
            A(2) = qq
            A(3) = -qq*A(1)
            if ( Lqmax.gt.3 ) then
               A(4) = qq*((A(1)+A(1))-A(2))
               A(5) = -qq*A(3)
               if ( Lqmax.gt.4 ) then
                  A(6) = qq*Three*A(2)
                  A(7) = qq*(Three*A(3)-A(4))
                  A(8) = -qq*A(5)
                  if ( Lqmax.gt.5 ) then
                     A(9) = qq*(Four*A(4)-A(6))
                     A(10) = qq*(Four*A(5)-A(7))
                     A(11) = -qq*A(8)
                     if ( Lqmax.gt.6 ) then
                        A(12) = qq*Five*A(6)
                        A(13) = qq*(Five*A(7)-A(9))
                        A(14) = qq*(Five*A(8)-A(10))
                        A(15) = -qq*A(11)
                     endif
                  endif
               endif
            endif
         endif
      endif
C
      if ( Lpmax.gt.1 ) then
         A(16) = Pp
         if ( Lqmax.gt.1 ) then
            A(17) = Pp*A(1)
            if ( Lqmax.gt.2 ) then
               A(18) = Pp*A(2)
               A(19) = Pp*A(3)
               if ( Lqmax.gt.3 ) then
                  A(20) = Pp*A(4)
                  A(21) = Pp*A(5)
                  if ( Lqmax.gt.4 ) then
                     A(22) = Pp*A(6)
                     A(23) = Pp*A(7)
                     A(24) = Pp*A(8)
                     if ( Lqmax.gt.5 ) then
                        A(25) = Pp*A(9)
                        A(26) = Pp*A(10)
                        A(27) = Pp*A(11)
                        if ( Lqmax.gt.6 ) then
                           A(28) = Pp*A(12)
                           A(29) = Pp*A(13)
                           A(30) = Pp*A(14)
                           A(31) = Pp*A(15)
                        endif
                     endif
                  endif
               endif
            endif
         endif
C
         if ( Lpmax.gt.2 ) then
            A(32) = Pp
            A(33) = +Pp*A(16)
            if ( Lqmax.gt.1 ) then
               A(34) = Pp*A(1)
               A(35) = +Pp*A(17)
               if ( Lqmax.gt.2 ) then
                  A(36) = Pp*A(2)
                  A(37) = Pp*(A(3)+A(18))
                  A(38) = +Pp*A(19)
                  if ( Lqmax.gt.3 ) then
                     A(39) = Pp*A(4)
                     A(40) = Pp*(A(5)+A(20))
                     A(41) = +Pp*A(21)
                     if ( Lqmax.gt.4 ) then
                        A(42) = Pp*A(6)
                        A(43) = Pp*(A(7)+A(22))
                        A(44) = Pp*(A(8)+A(23))
                        A(45) = +Pp*A(24)
                        if ( Lqmax.gt.5 ) then
                           A(46) = Pp*A(9)
                           A(47) = Pp*(A(10)+A(25))
                           A(48) = Pp*(A(11)+A(26))
                           A(49) = +Pp*A(27)
                           if ( Lqmax.gt.6 ) then
                              A(50) = Pp*A(12)
                              A(51) = Pp*(A(13)+A(28))
                              A(52) = Pp*(A(14)+A(29))
                              A(53) = Pp*(A(15)+A(30))
                              A(54) = +Pp*A(31)
                           endif
                        endif
                     endif
                  endif
               endif
            endif
C
            if ( Lpmax.gt.3 ) then
               A(55) = Pp*((A(16)+A(16))+A(32))
               A(56) = +Pp*A(33)
               if ( Lqmax.gt.1 ) then
                  A(57) = Pp*((A(17)+A(17))+A(34))
                  A(58) = +Pp*A(35)
                  if ( Lqmax.gt.2 ) then
                     A(59) = Pp*((A(18)+A(18))+A(36))
                     A(60) = Pp*((A(19)+A(19))+A(37))
                     A(61) = +Pp*A(38)
                     if ( Lqmax.gt.3 ) then
                        A(62) = Pp*((A(20)+A(20))+A(39))
                        A(63) = Pp*((A(21)+A(21))+A(40))
                        A(64) = +Pp*A(41)
                        if ( Lqmax.gt.4 ) then
                           A(65) = Pp*((A(22)+A(22))+A(42))
                           A(66) = Pp*((A(23)+A(23))+A(43))
                           A(67) = Pp*((A(24)+A(24))+A(44))
                           A(68) = +Pp*A(45)
                           if ( Lqmax.gt.5 ) then
                              A(69) = Pp*((A(25)+A(25))+A(46))
                              A(70) = Pp*((A(26)+A(26))+A(47))
                              A(71) = Pp*((A(27)+A(27))+A(48))
                              A(72) = +Pp*A(49)
                              if ( Lqmax.gt.6 ) then
                                 A(73) = Pp*((A(28)+A(28))+A(50))
                                 A(74) = Pp*((A(29)+A(29))+A(51))
                                 A(75) = Pp*((A(30)+A(30))+A(52))
                                 A(76) = Pp*((A(31)+A(31))+A(53))
                                 A(77) = +Pp*A(54)
                              endif
                           endif
                        endif
                     endif
                  endif
               endif
C
               if ( Lpmax.gt.4 ) then
                  A(78) = Pp*Three*A(32)
                  A(79) = Pp*(Three*A(33)+A(55))
                  A(80) = +Pp*A(56)
                  if ( Lqmax.gt.1 ) then
                     A(81) = Pp*Three*A(34)
                     A(82) = Pp*(Three*A(35)+A(57))
                     A(83) = +Pp*A(58)
                     if ( Lqmax.gt.2 ) then
                        A(84) = Pp*Three*A(36)
                        A(85) = Pp*(Three*A(37)+A(59))
                        A(86) = Pp*(Three*A(38)+A(60))
                        A(87) = +Pp*A(61)
                        if ( Lqmax.gt.3 ) then
                           A(88) = Pp*Three*A(39)
                           A(89) = Pp*(Three*A(40)+A(62))
                           A(90) = Pp*(Three*A(41)+A(63))
                           A(91) = +Pp*A(64)
                           if ( Lqmax.gt.4 ) then
                              A(92) = Pp*Three*A(42)
                              A(93) = Pp*(Three*A(43)+A(65))
                              A(94) = Pp*(Three*A(44)+A(66))
                              A(95) = Pp*(Three*A(45)+A(67))
                              A(96) = +Pp*A(68)
                              if ( Lqmax.gt.5 ) then
                                 A(97) = Pp*Three*A(46)
                                 A(98) = Pp*(Three*A(47)+A(69))
                                 A(99) = Pp*(Three*A(48)+A(70))
                                 A(100) = Pp*(Three*A(49)+A(71))
                                 A(101) = +Pp*A(72)
                                 if ( Lqmax.gt.6 ) then
                                    A(102) = Pp*Three*A(50)
                                    A(103) = Pp*(Three*A(51)+A(73))
                                    A(104) = Pp*(Three*A(52)+A(74))
                                    A(105) = Pp*(Three*A(53)+A(75))
                                    A(106) = Pp*(Three*A(54)+A(76))
                                    A(107) = +Pp*A(77)
                                 endif
                              endif
                           endif
                        endif
                     endif
                  endif
C
                  if ( Lpmax.gt.5 ) then
                     A(108) = Pp*(Four*A(55)+A(78))
                     A(109) = Pp*(Four*A(56)+A(79))
                     A(110) = +Pp*A(80)
                     if ( Lqmax.gt.1 ) then
                        A(111) = Pp*(Four*A(57)+A(81))
                        A(112) = Pp*(Four*A(58)+A(82))
                        A(113) = +Pp*A(83)
                        if ( Lqmax.gt.2 ) then
                           A(114) = Pp*(Four*A(59)+A(84))
                           A(115) = Pp*(Four*A(60)+A(85))
                           A(116) = Pp*(Four*A(61)+A(86))
                           A(117) = +Pp*A(87)
                           if ( Lqmax.gt.3 ) then
                              A(118) = Pp*(Four*A(62)+A(88))
                              A(119) = Pp*(Four*A(63)+A(89))
                              A(120) = Pp*(Four*A(64)+A(90))
                              A(121) = +Pp*A(91)
                              if ( Lqmax.gt.4 ) then
                                 A(122) = Pp*(Four*A(65)+A(92))
                                 A(123) = Pp*(Four*A(66)+A(93))
                                 A(124) = Pp*(Four*A(67)+A(94))
                                 A(125) = Pp*(Four*A(68)+A(95))
                                 A(126) = +Pp*A(96)
                                 if ( Lqmax.gt.5 ) then
                                    A(127) = Pp*(Four*A(69)+A(97))
                                    A(128) = Pp*(Four*A(70)+A(98))
                                    A(129) = Pp*(Four*A(71)+A(99))
                                    A(130) = Pp*(Four*A(72)+A(100))
                                    A(131) = +Pp*A(101)
                                    if ( Lqmax.gt.6 ) then
                                       A(132) = Pp*(Four*A(73)+A(102))
                                       A(133) = Pp*(Four*A(74)+A(103))
                                       A(134) = Pp*(Four*A(75)+A(104))
                                       A(135) = Pp*(Four*A(76)+A(105))
                                       A(136) = Pp*(Four*A(77)+A(106))
                                       A(137) = +Pp*A(107)
                                    endif
                                 endif
                              endif
                           endif
                        endif
                     endif
C
                     if ( Lpmax.gt.6 ) then
                        A(138) = Pp*Five*A(78)
                        A(139) = Pp*(Five*A(79)+A(108))
                        A(140) = Pp*(Five*A(80)+A(109))
                        A(141) = +Pp*A(110)
                        if ( Lqmax.gt.1 ) then
                           A(142) = Pp*Five*A(81)
                           A(143) = Pp*(Five*A(82)+A(111))
                           A(144) = Pp*(Five*A(83)+A(112))
                           A(145) = +Pp*A(113)
                           if ( Lqmax.gt.2 ) then
                              A(146) = Pp*Five*A(84)
                              A(147) = Pp*(Five*A(85)+A(114))
                              A(148) = Pp*(Five*A(86)+A(115))
                              A(149) = Pp*(Five*A(87)+A(116))
                              A(150) = +Pp*A(117)
                              if ( Lqmax.gt.3 ) then
                                 A(151) = Pp*Five*A(88)
                                 A(152) = Pp*(Five*A(89)+A(118))
                                 A(153) = Pp*(Five*A(90)+A(119))
                                 A(154) = Pp*(Five*A(91)+A(120))
                                 A(155) = +Pp*A(121)
                                 if ( Lqmax.gt.4 ) then
                                    A(156) = Pp*Five*A(92)
                                    A(157) = Pp*(Five*A(93)+A(122))
                                    A(158) = Pp*(Five*A(94)+A(123))
                                    A(159) = Pp*(Five*A(95)+A(124))
                                    A(160) = Pp*(Five*A(96)+A(125))
                                    A(161) = +Pp*A(126)
                                    if ( Lqmax.gt.5 ) then
                                       A(162) = Pp*Five*A(97)
                                       A(163) = Pp*(Five*A(98)+A(127))
                                       A(164) = Pp*(Five*A(99)+A(128))
                                       A(165) = Pp*(Five*A(100)+A(129))
                                       A(166) = Pp*(Five*A(101)+A(130))
                                       A(167) = +Pp*A(131)
                                       if ( Lqmax.gt.6 ) then
                                         A(168) = Pp*Five*A(102)
                                         A(169)
     &                                      = Pp*(Five*A(103)+A(132))
                                         A(170)
     &                                      = Pp*(Five*A(104)+A(133))
                                         A(171)
     &                                      = Pp*(Five*A(105)+A(134))
                                         A(172)
     &                                      = Pp*(Five*A(106)+A(135))
                                         A(173)
     &                                      = Pp*(Five*A(107)+A(136))
                                         A(174) = +Pp*A(137)
                                       endif
                                    endif
                                 endif
                              endif
                           endif
                        endif
                     endif
                  endif
               endif
            endif
         endif
      endif
C
      return
C
      end
@* INDEX.
