@a
      subroutine tread(IARN,X,M,N,MM,NN,K)
      implicit none
      integer IARN , ii , inum , K , M , MM , N , NN
      double precision X
      dimension X(M,N)
C
C1tread
C
C     subroutine tread(iarn,x,m,n,mm,nn,k)
C            'tread' reads specified data from a read-write
C     file, and performs any unpacking necessary to make
C     the dimensions of the input matrix agree with that
C     currently used (the matrix may not be filled to its maximum
C     size).  the calling arguments are (in order):
C
C     iarn ... this is the number of the rwf to be read.  a full
C              of the currently used file numbers is maintained
C              in link1.
C
C     x ...... this is the array into which data will be read.
C
C     m, n ... these are the actual dimensions of the array x.
C              (dimension x(m,n)).
C
C     mm, nn . these are the limits to which the array is to be
C              filled.
C
C     k ...... this flag indicates whether only the lower triangle
C              of a symmetric matrix was written out.
C              0 ... no, the full matrix is there.
C              1 ... yes.  in this case, the full matrix is
C                    generated by assuming the matrix to be
C                    symmetric.
C
C
C     ****************************************************************
C
C
C
C
C     temporary code to deal with new file numbers.
      inum = IARN
      if ( mod(inum,1000).lt.500 ) inum = inum + 500
C
      if ( K.ne.0 ) then
         ii = (MM*(MM+1))/2
      else
C
         ii = MM*NN
      endif
C     reads record x
      call fileio(2,-inum,ii,X,0)
      if ( NN.ne.1 ) call unpcck(K,ii,X,M,N,MM,NN)
      return
C
      end
