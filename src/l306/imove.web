@a
      subroutine imove(N,INVEC,OUTVEC)
      implicit none
      integer i
C
C     copy n elements of invec to outvec.
C
      integer N , INVEC , OUTVEC
      dimension INVEC(*) , OUTVEC(*)
      if ( N.lt.1 ) return
      do 100 i = 1 , N
         OUTVEC(i) = INVEC(i)
 100  continue
      return
      end
