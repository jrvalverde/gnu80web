@* fcorpr.
\section*{\sf Dummy Arguments}  \begin{description}
                                                                    
\item[INPUT:] NATOMS                                                    
\item[INPUT:] IAN                                                       
\item[INPUT:] F                                                         
\item[INPUT:] IOUT                                                      
                                                                    
\end{description} \section*{\sf Common Variables}  \begin{description}
                                                                    
\item[Modifies]     NOTHING                                           
\item[Uses]   NOTHING                                           
                                                                    
\end{description}  \section*{\sf External Subprograms}  \begin{description}
                                                                    
\item[Calls]        NOTHING                                           
\item[Called by]   FRCOUT                                                 
                                                                    
\end{description}
@a
      subroutine fcorpr(NATOMS,IAN,F,IOUT)
      implicit none
      double precision cloc , F
      integer i , iaind , IAN , iat , icent , IOUT , NATOMS
      dimension IAN(*) , F(*) , cloc(3)
C
C
C***********************************************************************
C     routine to print the contents of c as a 3*natoms array.
C
C     arguments:
C
C     natoms ... number of lines to print.
C     ian    ... array of length natoms, containing the atomic numbers.
C     f      ... array of length 3*natoms containing the (x,y,z)
C                things to print.  (like coordinates or forces).
C     iout   ... fortran logical unit for printing.
C***********************************************************************
C
C
C
C
99001 format (1x,58('-'))
99002 format (1x,'CENTER',5x,'ATOMIC',14x,' FORCES (HARTREES/BOHR)'/1x,
     &        'NUMBER',5x,'NUMBER',13x,'X',11x,'Y',11x,'Z')
99003 format (1x,i4,7x,i4,7x,3F12.6)
99004 format (1x,11x,i4,7x,3F12.6)
C
      write (IOUT,99001)
      write (IOUT,99002)
      write (IOUT,99001)
      icent = 0
      iaind = 0
      do 100 iat = 1 , NATOMS
         do 50 i = 1 , 3
            cloc(i) = F(i+iaind)
 50      continue
         if ( IAN(iat).lt.0 ) then
C
            write (IOUT,99004) IAN(iat) , cloc(1) , cloc(2) , cloc(3)
         else
            icent = icent + 1
            write (IOUT,99003) icent , IAN(iat) , cloc(1) , cloc(2) , 
     &                         cloc(3)
         endif
         iaind = iaind + 3
 100  continue
      write (IOUT,99001)
      return
C
      end
@* INDEX.
