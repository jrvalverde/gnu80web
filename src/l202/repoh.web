\end{description} \section*{\sf Common Variables}  \begin{description}
                                                                    
\item[Modifies]    ICNT    ISAVE   NSAVE                                  
\item[Uses]   NOTHING                                           
                                                                    
\end{description}  \section*{\sf External Subprograms}  \begin{description}
                                                                    
\item[Calls]       LABREP  PUTTBL                                         
\item[Called by]   INIREP                                                 
                                                                    
\end{description}
@a
      subroutine repoh
      implicit none
      integer Icnt , Isave , Nsave
C ----------------------------------------------------------------------
      double precision ohtbl(4,10)
      common /reploc/ Nsave , Icnt , Isave(15)
C      data ohtbl/'a1g',1.0,1.0,1.0,'a1u',1.0,1.0,-1.0,'a2g',1.0,-1.0,
C     +     1.0,'a2u',1.0,-1.0,-1.0,'eg',-1.0,0.0,2.0,'eu',-1.0,0.0,-2.0,
C     +     't1g',0.0,-1.0,3.0,'t1u',0.0,-1.0,-3.0,'t2g',0.0,1.0,3.0,
C     +     't2u',0.0,1.0,-3.0/
      data ohtbl/0.0 , 1.0 , 1.0 , 1.0 , 0.0 , 1.0 , 1.0 , -1.0 , 0.0 , 
     &     1.0 , -1.0 , 1.0 , 0.0 , 1.0 , -1.0 , -1.0 , 0.0 , -1.0 , 
     &     0.0 , 2.0 , 0.0 , -1.0 , 0.0 , -2.0 , 0.0 , 0.0 , -1.0 , 
     &     3.0 , 0.0 , 0.0 , -1.0 , -3.0 , 0.0 , 0.0 , 1.0 , 3.0 , 0.0 , 
     &     0.0 , 1.0 , -3.0/
C           routine to set up character table and initialize stuff for
C     molecules of oh symmetry.  there are 48 symmetry operations
C     in an octahedral molecule, and these are generated by "oper"
C     the order:
C
C             e, 9c4, 8c3, 6c2, i, 3sigmah, 6sigmad, 6s4, 8s6.
C
C     to assign orbital symmetries, we need save only the first
C     c3 axis, one c2 axis, and the inversion center.
C     r. a. whiteside - august 1979
C ----------------------------------------------------------------------
C
C                              character table for oh.
C
      Nsave = 3
      Icnt = 0
      Isave(1) = 10
      Isave(2) = 18
      Isave(3) = 24
      call puttbl(4,10,ohtbl)
      call labrep('A1G,A1U,A2G,A2U,EG,EU,T1G,T1U,T2G,T2U,',10)
      return
C
      end
@* INDEX.
