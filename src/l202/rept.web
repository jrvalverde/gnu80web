@* rept.
\end{description} \section*{\sf Common Variables}  \begin{description}
                                                                    
\item[Modifies]    ICNT    ISAVE   NSAVE                                  
\item[Uses]   NOTHING                                           
                                                                    
\end{description}  \section*{\sf External Subprograms}  \begin{description}
                                                                    
\item[Calls]       LABREP  PUTTBL                                         
\item[Called by]   INIREP                                                 
                                                                    
\end{description}
@a
      subroutine rept
      implicit none
      integer Icnt , Isave , Nsave
C ----------------------------------------------------------------------
      double precision ttbl(3,3)
      common /reploc/ Nsave , Icnt , Isave(15)
C      data ttbl/'a',1.0,1.0,'e',-1.0,2.0,'t',0.0,-1.0/
      data ttbl/0.0 , 1.0 , 1.0 , 0.0 , -1.0 , 2.0 , 0.0 , 0.0 , -1.0/
C           routine to set up character table and initialize stuff for
C     molecules of t symmetry.  there are 12 symmetry operations
C     in a tetrahedral molecule, and these are generated by "oper"
C     the order:
C
C         e, 8c3, 3c2,
C
C     to assign orbital symmetries, we need save only the first
C     c3 axis, and one c2 axis.
C     r. a. whiteside - august 1979
C ----------------------------------------------------------------------
C
C
      Nsave = 2
      Icnt = 0
      Isave(1) = 1
      Isave(2) = 9
      call puttbl(3,3,ttbl)
      call labrep('A,E,T,',3)
      return
C
      end
@* INDEX.
