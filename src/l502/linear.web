@a
      subroutine linear(A,B,MAX,N)
      implicit none
      double precision A , B
      integer i , j , k , MAX , N
      dimension A(MAX,MAX) , B(*)
C
C
C     places symmetric square array in linear form
C
C
      k = 1
      do 100 j = 1 , N
         do 50 i = 1 , j
            B(k) = A(i,j)
            k = k + 1
 50      continue
 100  continue
      return
C
      end
