C*****************************************************************************
      function equal(IA,IB,L)
C*****************************************************************************
      implicit none
      integer i , IA , IB , L
      logical equal
C
      dimension IA(L) , IB(L)
C
C  test if the first l elements of vectors ia and ib are equal:
C
      equal = .false.
      do 100 i = 1 , L
         if ( IA(i).ne.IB(i) ) goto 200
 100  continue
      equal = .true.
 200  return
      end
