@a
C*****************************************************************************
      function veclen(X,N,NDIM)
C*****************************************************************************
      implicit none
      integer i , N , NDIM
      double precision sum , veclen , X , zero
      dimension X(NDIM)
      data zero/0.0D0/
C
      sum = zero
      do 100 i = 1 , N
         sum = sum + X(i)*X(i)
 100  continue
      veclen = sqrt(sum)
      return
      end
